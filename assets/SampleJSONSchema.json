{  
  "$schema":"http://json-schema.org/draft-04/schema#",
  "id":"Record",
  "type":"object",
  "properties":{  
    "operation":{  
      "id":"Operation",
      "type":"object",
      "properties":{  
        "operationId":{  
          "type":"integer",
          "format":"uint32",
          "description":"this can be used to extract semantic of this object"
        },
        "operationNumber":{  
          "type":"integer",
          "format":"uint32",
          "description":"this can be used to extract semantic of this object"
        }
      },
      "required":[  
        "operationId",
        "operationNumber"
      ]
    },
    "catch":{  
      "id":"Catch",
      "type":"object",
      "properties":{  
        "catchId":{  
          "type":"integer",
          "format":"uint32",
          "description":"this can be used to extract semantic of this object"
        },
        "catchNumber":{  
          "type":"integer",
          "format":"uint32",
          "description":"this can be used to extract semantic of this object"
        },
        "timestamp":{  
          "type":"integer",
          "format":"date-time"
        },
        "location":{  
          "type":"object",
          "properties":{  
            "lat":{  
              "type":"number"
            },
            "lng":{  
              "type":"number"
            }
          }
        },
        "speciesId":{  
          "type":"integer",
          "format":"uint32"
        },
        "speciesAge":{  
          "type":"integer",
          "format":"uint32"
        },
        "speciesMaturity":{  
          "type":"string"
        },
        "speciesSex":{  
          "enum":[  
            "male",
            "female",
            "unisex?",
            "homosexual??"
          ]
        },
        "speciesWeight":{  
          "type":"number",
          "format":"double",
          "minimum":0
        },
        "speciesLength":{  
          "type":"string",
          "format":"double",
          "minimum":0
        }
      },
      "required":[  
        "catchId",
        "catchNumber",
        "timestamp",
        "location",
        "speciesId",
        "speciesLength",
        "speciesWeight"
      ]
    },
    "vessel":{  
      "id":"Vessel",
      "type":"object",
      "properties":{  
        "vesselId":{  
          "type":"integer",
          "format":"uint32"
        },
        "vesselName":{  
          "type":"string"
        }
      },
      "required":[  
        "vesselId",
        "vesselName"
      ]
    },
    "permit":{  
      "id":"Permit",
      "type":"object",
      "properties":{  
        "permidId":{  
          "type":"integer",
          "format":"uint32"
        }
      },
      "required":[  
        "permidId"
      ]
    }
  },
  "required":[  
    "operation",
    "catch",
    "vessel",
    "permit"
  ]
}